<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ThickProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_progress_bar</item>
        <item name="android:progressTint">@color/gold</item>




    </style>
    <style name="ButtonStyle" parent="Widget.AppCompat.Button">
        <!-- Customize your button style attributes here -->
        <item name="android:background">@color/gold</item>
        <item name="android:textColor">@color/white</item>
        <!-- Add more attributes as needed -->
    </style>
    <style name="ItemCountBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_progress_bar</item>
        <item name="android:progressTint">@color/gold</item>


    </style>
    <style name="CustomChipStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <!-- Customize chip attributes here -->
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Medium</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/black</item>

        <!-- Add more custom attributes as needed -->
    </style>



</resources>