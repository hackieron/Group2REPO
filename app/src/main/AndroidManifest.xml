<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_ASSETS" />
    <uses-permission
        android:name="android.permission.INTERNET"
        android:maxSdkVersion="34"/>
    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE"
        android:maxSdkVersion="34" />
    <uses-permission
        android:name="android.permission.READ_MEDIA_IMAGES"
        android:maxSdkVersion="34" />




    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CourSeer2"
        tools:overrideLibrary="com.android.car.ui"
        tools:targetApi="34">

        <activity
            android:name=".FullDescriptionActivity"
            android:exported="false" />
        <activity
            android:name=".SFullDescriptionActivity"
            android:exported="false" />
        <activity
            android:name=".UserView"
            android:exported="false" />
        <activity
            android:name=".Careers"
            android:exported="false" />
        <activity
            android:name=".UserCreate"
            android:exported="false" />
        <activity
            android:name=".Interests"
            android:exported="false" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>